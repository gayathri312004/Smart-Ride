# Production Environment Variables for Server
# MongoDB Connection
MONGODB_URI=your-mongodb-atlas-connection-string

# Frontend URL (will be set to your deployed frontend URL)
FRONTEND_URL=https://your-frontend-url.railway.app

# Google OAuth Callback (will be set to your deployed backend URL)
GOOGLE_CALLBACK_URL=https://your-backend-url.railway.app/api/auth/google/callback

# Server Environment
NODE_ENV=production
PORT=5000

# Session Secret (use a secure random string in production)
SESSION_SECRET=your-super-secure-session-secret-for-production

# Google OAuth (if used)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# JWT Secret (if used)
JWT_SECRET=your-jwt-secret-for-production
